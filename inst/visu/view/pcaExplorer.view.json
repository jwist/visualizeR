              "script": [
                "let marker = API.getData('selectedMarker')\n\nlet data = API.getData('data')\n\nlet selectedMarkers = marker.map( r => r._highlight[0] )\n\nlet selectedData = data.filter( (val, ind, arr) => \n    {return selectedMarkers.includes(val._highlight)} )\n\nlet x = selectedData[0].dataMatrix.filter((val, idx, Arr) => \n        idx % 2 == 0\n    )\n\n//let col = []\n//x.forEach( i => col.push({'_rgb':[0,0,255,1]}) )\n\nlet y = data.map( x => \n    x.dataMatrix.filter((val, idx, Arr) => \n        idx % 2 == 1\n    )\n)\nconsole.log('exec')\n\nlet color = [];\nlet spectra = selectedData.map((e, i) => {\n    let groupColor = e.color.split(\"(\")[1].split(\")\")[0].split(\",\").map(Number)\n    color = []\n    x.forEach( () => color.push({'_rgb': groupColor}) )\n    let s = ({\n        \"type\": 'color', //set to line in new visualizer\n        \"label\": e.ID,\n        \"styles\": {\n            \"unselected\": {\n                \"lineColor\": \"blue\",\n                \"lineWidth\": 3,\n                \"lineStyle\": 4\n            },\n            \"selected\": {\n                \"lineColor\": \"blue\",\n                \"lineWidth\": 5,\n                \"lineStyle\": 4\n            }\n        },\n        \"y\": y[i],\n        \"x\": x,\n        \"color\": color //remove with new visualizer\n        })\n    return s\n    }\n)\n\nAPI.createData('selectedData', selectedData)\nAPI.createData('selectedIndex', selectedMarkers)\nAPI.createData('selectedSpectra', {'data':spectra})\nAPI.createData('modifiedSelectedSpectra', {'data':spectra})\n\n"
              ]
